-- Challenge 1, solution 1
type solve ((Itr Int)*, (Itr Int)*) -> NoneType;
func solve (*[], *bs) = None;
func solve (*[a], *bs) = out([a]);
func solve (*(a1:a2:as), *(b:bs)) = {
    out([a1,a2,b]);
    solve(as, bs);
};

setIn([0,1]);

