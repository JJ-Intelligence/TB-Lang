-- Challenge 8

try {
    -- Keeps looping until the streams run out of values
    while (True) {
        for ({ i = 0; sum = 0 }; i < 5; i++) {
            a = pop(in(0));
            b = pop(in(1));
            sum += (a - b);
            out([a, b])
        };
        out(sum);
    };
} catch (StreamOutOfInputException) { 0 };