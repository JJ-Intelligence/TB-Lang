
type solve ((Itr Int)*, (Itr Int)*) -> NoneType;
func solve (*[], *bs) = None; 
func solve (*[a], *bs) = out(a); 
func solve (*(x:y:as), *(z:bs)) = {
    out ([x,y,z]);
    solve (as, bs);
};

setIn([1,0]);

as = in(0);
bs = in(1);

solve (as,bs);


